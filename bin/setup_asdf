# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# APP Variables overrides
export ASDF_DIR="$SETUP_ASDF_DIR" ASDF_DATA_DIR="$SETUP_ASDF_DATA_DIR"
