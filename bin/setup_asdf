# Actions based on env
[ -d "$SETUP_ASDF_DIR/.git" ] || __setup_asdf
__asdf_init
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# begin main app
if [ "$1" = "enable-all" ] || [ "$SETUP_ASDF_ENABLE_ALL" = "true" ]; then
  shift $#
  __install_all_plugins
  exit $?
elif [ $# -ne 0 ]; then
  LISTARRAY="$*"
  for pkg in $LISTARRAY; do
    __asdf_plugin "$pkg"
  done
